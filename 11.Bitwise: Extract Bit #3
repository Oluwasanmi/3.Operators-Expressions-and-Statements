using System;

class DisplayBitByGivenNumber
{
    static void Main()
    {
        Console.Write("Enter number n:");
        uint p = 3;
        uint n = uint.Parse(Console.ReadLine());               
        uint nRightP =(n >>(int) p);      
        uint bit = nRightP & 1;     
        Console.WriteLine(bit);    
    }
}

