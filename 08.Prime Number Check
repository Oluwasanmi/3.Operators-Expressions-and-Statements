using System;

class Prime
{
    static void Main()
    {
        Console.WriteLine("Enter number:");
        int n = int.Parse(Console.ReadLine());
        if (n < 2|| n > 100)
        {
            Console.WriteLine(false);
            return;
        }
        int i = 1;
        int count = 0;
        while (n>=i)
        {
            if (n%i==0)
            {
                count++;
            }
            i++;
        }
        if (count>2)
        {
            Console.WriteLine(false);
        }
        else
        {
            Console.WriteLine(true);
        }
    }
}

