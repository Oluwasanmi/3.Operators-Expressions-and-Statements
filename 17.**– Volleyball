using System;

class Volleyball
{
    static void Main()
    {
        string kindYear = Console.ReadLine();
        int p = int.Parse(Console.ReadLine());
        int h = int.Parse(Console.ReadLine());
        double year = h + ((48 - h) * 3.0 / 4) + (p * 2.0 / 3);
        if (kindYear=="leap")
        {
            Console.WriteLine("{0}",(int) (year + (0.15 * year)));
        }
        else
        {
            Console.WriteLine("{0}", (int)(year));
        }
    }
}

