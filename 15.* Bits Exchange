using System;
class ExchangeOfThreeBits
{
    static void Main()
    {
      Console.WriteLine("Enter number:");
      uint n=uint.Parse(Console.ReadLine());
      for (int P = 3,Q=24; P <=5||Q<=26; P++,Q++)
      {
          int mask;
          uint bitP = (n>>P) & 1;
          uint bitQ = (n >> Q) & 1;

          if (bitQ == 0) //put bitP=0;
          { 
             mask = ~(1 << P);
              n = n & (uint)mask;
          }
          else if (bitQ == 1)//put bitP=1;
          {
              mask = 1 <<P;
              n = n|(uint)mask;
          }
          if (bitP == 0)//put bitQ=0;
          {
              mask = ~(1 << (int)Q);
              n = n & (uint)mask;
          }
          else if (bitP == 1)//put bitQ=1;
          {
              mask = 1 << (int)Q;
              n= n | (uint)mask;
          }
      }
      Console.WriteLine(n);
    }
}

