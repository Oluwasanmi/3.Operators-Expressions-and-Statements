using System;

class ThirdDigitIs7
{
    static void Main()
    {
        Console.WriteLine("Enter the three digit number:");
        int n = int.Parse(Console.ReadLine());
        if (n<700)
        {
            Console.WriteLine(false);
        }
        else
        {
            int hundred = n / 10;
            int tenths = hundred / 10;
            if (tenths % 10 == 7)
            {
                Console.WriteLine(true);
            }
            else
            {
                Console.WriteLine(false);
            }
        }
    }
}

