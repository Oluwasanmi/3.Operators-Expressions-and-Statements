using System;

class DivideBy7And5
{
    static void Main()
    {
        Console.WriteLine("Enter number:");
        int intNum = int.Parse(Console.ReadLine());
        if (intNum%7==0&&intNum%5==0&&intNum!=0)
        {
            Console.WriteLine(true);
        }
        else
        {
            Console.WriteLine(false);
        }
    }
}

